      <h2>Wedding Party</h2>
      <div class="body-container">
        <div class="body">
          <div id="wedding-party-body">
            <div id="image-carousel">
              <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
                <!-- Indicators -->
                <ol class="carousel-indicators">
